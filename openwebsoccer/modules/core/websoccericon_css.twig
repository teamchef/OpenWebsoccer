#report_result { border-top: 1px solid #000000; border-bottom: 1px solid #000000; text-align: center; }
#report_goals_home, #report_goals_separator, #report_goals_guest { padding: 30px; display: inline-block; font-size: 36px; font-weight: bold; }
#report_goals_home { padding-left: 0; }
#report_goals_guest { padding-right: 0; }
#reportarea { border: 1px solid #000000; padding: 5px; border: 1px solid #dde2e4; -webkit-border-radius: 4px; -moz-border-radius: 4px; border-radius: 4px; -webkit-box-shadow: inset 0 1px 1px rgb(0, 0, 0); -moz-box-shadow: inset 0 1px 1px rgb(0, 0, 0); box-shadow: inset 0 1px 1px rgb(0, 0, 0); }
.report_minute { font-weight: bold; color: #000000; }
.report_comment { color: #666666; }
.report_team_home { color: #006699; }
#reporttable { table-layout: fixed; }
#reporttable td { padding-top: 10px; }
.report_team_guest { color: #ff6600; }
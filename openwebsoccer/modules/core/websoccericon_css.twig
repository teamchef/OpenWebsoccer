{% if env.getConfig("cdn_root_set") %}
	.websoccericon-shoot { background: url('{{ env.getConfig("cdn_root") }}/img/icons-websoccer-32.png') no-repeat -95px -225px; width: 34px; height: 32px; display: inline-block; }
